# overwrite protection
alias mv="mv -i"
alias cp="cp -i"

# file listing
alias l="ls -alF"

# folder navigation
alias ..="cd .."
alias ...="cd  ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ......="cd ../../../../.."

# directory back
alias 1="cd -"
alias 2="cd -2"
alias 3="cd -3"
alias 4="cd -4"
alias 5="cd -5"
alias 6="cd -6"
alias 7="cd -7"
alias 8="cd -8"
alias 9="cd -9"

# fun
alias sayit="Software is like sex: it's better when it's free - Linus Torvalds"
